          "oauthConfiguration": {
            "authorizedRedirectURLs": [
              "http://localhost:5000/callback"
            ],
            "logoutURL": "http://localhost:5000"
          },
          "registrationConfiguration": {
            "enabled": true,
            "type": "advanced",
            "formId": "#{registrationFormId}"
          },
          "roles": [
            {
              "name": "admin"
            },
            {
              "name": "user"
            }
          ]
        }
      }
    },
    {
      "method": "POST",
