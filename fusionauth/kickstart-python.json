    },
    {
      "key": "43d3eca9b0664a929f64b67230898d9516a5ecf0fa1d446f8122c8d6a5cc2709",
      "description": "python flask oauth API key",
      "tenantId": "#{FUSIONAUTH_TEANANT_ID}",
      "permissions": {
        "endpoints": {
          "/api/user/registration": [
            "GET",
            "POST",
            "PUT",
            "PATCH",
            "DELETE"
          ],
          "/api/form": [
            "GET"
          ],
          "/api/form/field": [
            "GET"
          ]
